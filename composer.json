{
    "name": "hzerbini/unit-conversions",
    "description": "This is my package unit-conversions",
    "keywords": [
        "hzerbini",
        "unit-conversions"
    ],
    "homepage": "https://github.com/hzerbini/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Henrique Zerbini Junqueira de Assis",
            "email": "henriquezjassis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Hzerbini\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hzerbini\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
